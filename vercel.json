{
  "buildCommand": "cd fixology-next && npm ci && npm run build",
  "outputDirectory": "fixology-next/.next",
  "installCommand": "cd fixology-next && npm ci",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}
