{
  "version": 2,
  "builds": [
    {
      "src": "api/imei-check.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "api/fixology-chat.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "api/shops.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "api/pricing.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "api/submit-price.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "api/devices.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/imei-check",
      "dest": "/api/imei-check.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/fixology-chat",
      "dest": "/api/fixology-chat.js",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/shops",
      "dest": "/api/shops.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/pricing",
      "dest": "/api/pricing.js",
      "methods": ["GET", "POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/submit-price",
      "dest": "/api/submit-price.js",
      "methods": ["POST", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "src": "/api/devices",
      "dest": "/api/devices.js",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ]
}
